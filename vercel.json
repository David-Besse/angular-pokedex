{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "https://dbwd-pokedex-api.vercel.app",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://dbwb-pokedex.vercel.app"
                },
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": "true"
                }
            ]
        }
    ]
}